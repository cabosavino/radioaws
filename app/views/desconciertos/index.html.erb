<p id="notice"><%= notice %></p>

<h1>Desconciertos</h1>

<table>
  <thead>
    <tr>
      <th>At date</th>
      <th>Url1</th>
      <th>Url2</th>
      <th>Url3</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @desconciertos.each do |desconcierto| %>
      <tr>
        <td><%= desconcierto.at_date %></td>
        <td><%= desconcierto.url1 %></td>
        <td><%= desconcierto.url2 %></td>
        <td><%= desconcierto.url3 %></td>
        <td><%= link_to 'Show', desconcierto %></td>
        <td><%= link_to 'Edit', edit_desconcierto_path(desconcierto) %></td>
        <td><%= link_to 'Destroy', desconcierto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Desconcierto', new_desconcierto_path %>
